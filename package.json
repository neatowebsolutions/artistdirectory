{
  "name": "root",
  "private": true,
  "workspaces": [
    "applications/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "lint": "lerna run lint",
    "format": "lerna run format",
    "bootstrap": "dotenv -- lerna bootstrap --force-local",
    "postbootstrap": "dotenv -- lerna run build --stream --scope=@artistdirectory/*",
    "prestart": "yarn install && dotenv -- yarn bootstrap",
    "start": "dotenv -- lerna run start --parallel --scope={@artistdirectory/*,artistdirectory-directory,artistdirectory-logs-service,artistdirectory-directory-api,artistdirectory-directory-api-gateway}"
  },
  "contributors": [
    "riemersmajackie@gmail.com",
    "hello@bethanypaquette.com",
    "todd.vanhouten@fusionary.com",
    "chad@neatowebsolutions.com"
  ],
  "license": "UNLICENSED",
  "homepage": "https://github.com/neatowebsolutions/artistdirectory",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.3",
    "lerna": "^3.22.1",
    "prettier": "^2.2.1",
    "webpack": "^5.21.0",
    "webpack-cli": "^4.5.0"
  }
}
